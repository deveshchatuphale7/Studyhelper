
<resources>
    <!-- Customizations -->

    <color name="textColor">#1c1818</color>
    <color name="textColorSecondary">#991c1818</color>
    <color name="textColorMuted">#4d1c1818</color>
    <color name="textBarColor">#b2b1b1</color>
    <color name="window_background">#6b6b6b</color>
   
    <!-- background drawable: be sure it has a not too large size/resolution -->
    <drawable name="activity_background_image">@drawable/jpg_images916</drawable>
    
    <!-- toolbar background -->
    <color name="actionbar_background">#8b611d</color>

    <!-- status bar background (android 5.x) -->
    <color name="primaryDark">#ff684815</color>
    
    <!-- list selections -->
    <color name="selectionColor">#441c1818</color>

    <!-- card style -->
    <drawable name="background_card">@drawable/bg_dark_card</drawable>

    <!-- card view -->
    <color name="cardBackgroundColor">#996b6b6b</color>

    <!-- icons -->
    <drawable name="ic_device_access_call">@drawable/ic_device_access_call_light</drawable>
    <drawable name="ic_location_web_site">@drawable/ic_location_web_site_light</drawable>
    <drawable name="ic_location_place">@drawable/ic_location_place_light</drawable>
    <drawable name="ic_content_email">@drawable/ic_content_email_light</drawable>

</resources>

